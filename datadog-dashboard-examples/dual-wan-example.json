{
    "title":"Mikrotik Router with Dual WAN",
    "description":"Dashboard for a router with ether1 and ether2 used as uplink WAN connectivity",
    "widgets":[
       {
          "id":123456789001,
          "definition":{
             "type":"timeseries",
             "requests":[
                {
                   "q":"avg:system.cpu.load{$host}",
                   "display_type":"line",
                   "style":{
                      "palette":"dog_classic",
                      "line_type":"solid",
                      "line_width":"normal"
                   }
                }
             ],
             "custom_links":[
                
             ],
             "yaxis":{
                "min":"0",
                "max":"100"
             },
             "markers":[
                {
                   "value":"y = 30",
                   "display_type":"warning dashed"
                },
                {
                   "value":"40 < y < 100",
                   "display_type":"error dashed",
                   "label":"Error threshold"
                }
             ],
             "title":"System CPU Load",
             "title_size":"13",
             "title_align":"center",
             "show_legend":true,
             "legend_size":"0"
          },
          "layout":{
             "x":1,
             "y":20,
             "width":47,
             "height":15
          }
       },
       {
          "id":123456789002,
          "definition":{
