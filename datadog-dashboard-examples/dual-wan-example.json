                   "metadata":[
                      {
                         "expression":"sum:system.memory.free{$host}",
                         "alias_name":"Free Memory"
                      },
                      {
                         "expression":"sum:system.memory.total{$host}-sum:system.memory.free{$host}",
                         "alias_name":"Used Memory"
                      }
                   ],
                   "display_type":"area",
                   "style":{
                      "palette":"orange",
                      "line_type":"solid",
                      "line_width":"normal"
                   }
                }
             ],
             "custom_links":[
                
             ],
